{
	"name": "Unregistered Edit Links",
	"version": "2.2.1",
	"author": [
		"alex4401"
	],
	"url": "https://github.com/alex4401/mediawiki-extensions-UnregisteredEditLinks",
	"descriptionmsg": "unregisterededitlinks-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.41.0",
		"platform": {
			"php": ">= 8.1"
		}
	},
	"MessagesDirs": {
		"UnregisteredEditLinks": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\UnregisteredEditLinks\\": "src/"
	},
	"ServiceWiringFiles": [
		"src/ServiceWiring.php"
	],
	"HookHandlers": {
		"Main": {
			"class": "MediaWiki\\Extension\\UnregisteredEditLinks\\Hooks",
			"services": [
				"UnregisteredEditLinks"
			]
		},
        "VisualEditorHooks": {
            "class": "MediaWiki\\Extension\\UnregisteredEditLinks\\VisualEditorHooks",
			"services": [
				"UnregisteredEditLinks"
			]
        }
	},
	"Hooks": {
		"SkinTemplateNavigation::Universal": "Main",
		"LoginFormValidErrorMessages": "Main",
        "VisualEditorBeforeEditor": "VisualEditorHooks"
	},
	"config": {
		"ForceVe": {
			"value": false
		},
		"Groups": {
			"value": [
				"user",
				"autoconfirmed",
				"emailconfirmed"
			]
		}
	},
	"config_prefix": "wgUnregisteredEditLinks",
	"manifest_version": 2
}
